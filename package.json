{
  "name": "stability-ai-api",
  "version": "0.1.3",
  "description": "Stability AI API wrapper - Stable Diffusion image generation for Node.js with CLI",
  "main": "api.js",
  "type": "module",
  "bin": {
    "sai": "cli.js"
  },
  "exports": {
    ".": "./api.js",
    "./api": "./api.js",
    "./utils": "./utils.js",
    "./config": "./config.js"
  },
  "files": [
    "api.js",
    "cli.js",
    "utils.js",
    "config.js",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "sai": "node cli.js",
    "sai:help": "node cli.js --help",
    "sai:examples": "node cli.js --examples",
    "sai:credits": "node cli.js credits",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "stability-ai",
    "stable-diffusion",
    "sd3",
    "stable-image",
    "image-generation",
    "ai-image",
    "text-to-image",
    "image-to-image",
    "upscale",
    "cli"
  ],
  "author": "img-gen Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aself101/stability-ai-api.git"
  },
  "bugs": {
    "url": "https://github.com/aself101/stability-ai-api/issues"
  },
  "homepage": "https://github.com/aself101/stability-ai-api#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "commander": "^11.1.0",
    "dotenv": "^16.3.1",
    "form-data": "^4.0.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "vitest": "^4.0.9",
    "@vitest/coverage-v8": "^4.0.9"
  }
}
